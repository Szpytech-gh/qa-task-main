{
  "name": "qa-tic-tac-toe",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "playwright test",
    "test:headed": "playwright test --headed",
    "test:headed:chrome": "playwright test --headed --project='chromium' --workers=1",
    "test:headed:chrome:core": "playwright test core-functionality.spec.ts --headed --project='chromium' --workers=1",
    "test:headed:mobile:chrome": "playwright test --headed --project='Mobile Chrome' --workers=1",
    "test:ui": "playwright test --ui",
    "test:debug": "playwright test --debug",
    "test:core": "playwright test tests/core-functionality.spec.ts",
    "test:report": "playwright show-report"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@playwright/test": "^1.53.1",
    "@types/node": "^24.0.3",
    "@types/react": "^18.3.17",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.17.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "globals": "^15.13.0",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.18.1",
    "vite": "^6.0.3"
  }
}
